{"ast":null,"code":"export default {\n  name: \"checkoutPage\",\n  props: [\"cart\"],\n  data() {\n    return {\n      name: \"\",\n      address: \"\"\n    };\n  },\n  methods: {\n    removeProduct(index) {\n      this.$emit(\"removeProduct\", index);\n    }\n  }\n};","map":{"version":3,"names":["name","props","data","address","methods","removeProduct","index","$emit"],"sources":["src/components/checkout.vue"],"sourcesContent":["<template>\n    <div>\n        <h2>Checkout</h2>\n        <h3>Added products</h3>\n        <div v-for=\"(product, index) in cart\" :key=\"index\">\n            <figure>\n                <img width=\"500\" height=\"400\" v-bind:src=\"product.image\" />\n            </figure>\n            <p>Subject: {{ product.title }}</p>\n            <p>Location: {{ product.location }}</p>\n            <p>Price: {{ product.price }}</p>\n            <button @click=\"removeProduct(index)\">remove</button>\n        </div>\n        <p>\n            <strong> Name </strong>\n            <input v-model=\"name\" />\n            <strong> Address </strong>\n            <input v-model=\"address\" />\n            <button> Submit </button>\n        </p>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: \"checkoutPage\",\n    props: [\"cart\"],\n    data() {\n        return {\n            name: \"\",\n            address: \"\"\n        }\n    },\n    methods: {\n        removeProduct(index) {\n            this.$emit(\"removeProduct\", index)\n        }\n    }\n}\n</script>"],"mappings":"AAwBA;EACAA,IAAA;EACAC,KAAA;EACAC,KAAA;IACA;MACAF,IAAA;MACAG,OAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAAC,KAAA;MACA,KAAAC,KAAA,kBAAAD,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}